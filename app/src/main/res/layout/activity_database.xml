<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Database"
    android:background="@drawable/og">

    <EditText
        android:id="@+id/editText501"
        android:layout_width="381dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="286dp"
        android:layout_marginEnd="3dp"
        android:ems="10"
        android:hint="Enter your name"
        android:inputType="textPersonName"
        android:textColor="#ECEC56"
        android:textColorHint="#E6CA65"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="127dp" />

    <EditText
        android:id="@+id/editText502"
        android:layout_width="383dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="4dp"
        android:ems="10"
        android:hint="Enter your password"
        android:inputType="textPersonName"
        android:textColor="#E6D462"
        android:textColorHint="#E6D26B"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="196dp" />

    <EditText
        android:id="@+id/editText503"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="122dp"
        android:layout_marginEnd="-4dp"
        android:ems="10"
        android:hint="Enter your mail id"
        android:inputType="textPersonName"
        android:textColor="#EACB61"
        android:textColorHint="#E6CA65"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="263dp" />

    <EditText
        android:id="@+id/editText504"
        android:layout_width="376dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="314dp"
        android:ems="10"
        android:hint="Enter your mob no."
        android:inputType="textPersonName"
        android:textColor="#E8CA60"
        android:textColorHint="#E8D765" />

    <Button
        android:id="@+id/button444"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="152dp"
        android:layout_marginBottom="217dp"
        android:background="#60E1CC"
        android:text="Submit" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="29dp"
        android:text="Registration"
        android:textColor="#C47DC668"
        android:textSize="20sp" />

    <Button
        android:id="@+id/button333"
        android:layout_width="103dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="147dp"
        android:layout_marginBottom="138dp"
        android:background="#55EAC7"
        android:text="Proceed" />

    <Button
        android:id="@+id/button555"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/flag_transparent"
        android:text="Back"
        android:textColor="#F10606"
        android:textSize="20sp" />
</RelativeLayout>